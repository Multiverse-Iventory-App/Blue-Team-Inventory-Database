<div>
	<ul >
		 <li >
            <article class="card">
                <img class="item-img" src="{{warehouse.image}}"/>
			    <h2>Name: {{warehouse.name}}</h2>
			    <h3>Location: {{warehouse.location}}</h3>
			    <h3>Max Pallete Capacity: <span id="max-capacity">{{warehouse.capacity}}</span></h3>
				<h3>Current Pallete Count: <span id="current-capacity">{{warehouse.Palletes.length}}</span></h3>
				<h3>Available Pallete Capacity: <span id="available-capacity"></span></h3>
                <br>
		    </article>
        </li>
    </ul>
	<div class='button-list'>
        <button><a href="/new-pallete/{{warehouse.id}}">Add New Pallete!</a></button>
		<button id="goBack-btn"><a class="main-button" href="/warehouses">Go Back to Warehouses List</a></button>
	</div>
	<br>
	<br>
</div>
{{#each warehouse.Palletes}} 
		<ul>
            <li>
                <article class="card">
			        <h3>Max Capacity: {{capacity}}</h3>
                    <h3>Pallete Id: {{id}}</h3>
                    <button id ="view-boxes"><a href="/palletes/{{id}}">View boxes</a></button>
		        </article>
            </li>
        </ul>
	{{/each}}
	 

<script src="/warehouse.js"></script>